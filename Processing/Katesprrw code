void setup() {
  size(500, 400);
  background(255, 255, 255);
}

void draw() {
  if (mousePressed) {
    background(0, 0, 0);
  } 

  stroke(0, 205, 255);
  fill(10, 20, 90);
  ellipse(mouseX, 200, 300, 300);

  fill(160, 210, 230);
  rect(245, mouseY, 10, 240);

  fill(255, 255, 255);
  ellipse(mouseX, mouseY, 70, 70);
  
  stroke(0, 25, 135);
  fill(60, 120, 190);
  ellipse(mouseX, 100, 300, 100);
  
 
}
